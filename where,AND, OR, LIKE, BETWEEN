SELECT * FROM customers WHERE name = 'Rahul';
SELECT * FROM customers WHERE name LIKE 'R%';
SELECT * FROM products WHERE price > 500;
SELECT * FROM products WHERE price BETWEEN 200 AND 1000;
SELECT * FROM products WHERE stock < 10 OR price > 1000;
SELECT * FROM products WHERE price > 500 AND stock > 50;
SELECT * FROM customers WHERE email LIKE '%@gmail.com';
